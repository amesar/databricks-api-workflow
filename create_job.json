{
  "name": "workflowApiClient_run_job",
  "new_cluster": {
    "spark_version": "5.0.x-scala2.11",
    "node_type_id": "i3.xlarge",
    "num_workers": 1,
    "driver_node_type_id": "i3.xlarge",
    "cluster_log_conf": {
      "dbfs": {
          "destination": "dbfs:/andre/logs/jobs/workflowApiClient_run_now"
      }
    }
  },
  "email_notifications": {
    "on_start": ["andre@databricks.com"],
    "on_success": ["andre@databricks.com"],
    "on_failure": ["andre@databricks.com"]
  },
  "libraries": [
    {
      "jar": "dbfs:/andre/jars/amm-hellofelidae_2.11-0.1-SNAPSHOT.jar"
    }
  ],
  "spark_jar_task": {
    "main_class_name": "org.andre.HelloFelidae",
    "parameters": [ "tiger" ]
  },
  "timeout_seconds": 3600,
  "max_retries": 0
}
